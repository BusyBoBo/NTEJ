		})
	})
	$('#activitiPicture').on('tap',function(){
		var mask = $('<div class="mui-backdrop"><img src="'+ip+'activitiPicture/'+processCode+'.png" /></div>')
		$('body').append(mask);
		$('.mui-backdrop').on('tap',function(){
			$('.mui-backdrop').remove()
		})
		//监听返回键
		var old_back = mui.back;
		mui.back = function(){
			if($('.mui-backdrop').is(":visible")){
				$('.mui-backdrop').remove()
			}else{
				old_back()
			}
		}
	})
})
</script>

</html>