	}
	
	.header-left {
		width: 33.3%;
		padding-left: 10px;
	}
	
	.header-left a {
		color: #EEEEEE;
		line-height: 60px;
	}
	
	.header-left img {
		width: 45px;
		border-radius: 100%;
		vertical-align: middle;
	}
	
	.header-center {
		width: 33.3%;
		font-size: 16px;
	}
	
	.header-right {
		width: 33.3%;
		text-align: right;
		padding-right: 20px;
		position: relative;
	}
	
	.header-right .mui-icon-extra {
		line-height: 60px;
		font-size: 20px;
	}
	
	.header-right-down {
		width: 120%;
		position: absolute;
		z-index: 9999;
		top: 50px;
		right: 1px;
		color: #000000;
		text-align: left;
	}
	
	.header-right-down li {
		height: 45px;
		line-height: 45px;
		background: #4177c7;
		color: #EEEEEE;
		text-align: center;
		border-radius: 2px;
		border-bottom: 1px solid #FFFFFF;
		padding-right: 5px;
	}
	
	.header-right-down li span {
		padding: 0 5px;
		vertical-align: middle;
	}
	
	.header-left img:hover {
		animation: roate360 0.5s;
	}
	
	.header-right span.mui-icon-plus:active {
		animation: roate360 0.5s infinite;
	}
	
	.page-main {
		margin-top: 48px;
	}
	/*css3动画*/
	
	@keyframes roate360 {
		from {
			transform: rotate(0deg);
		}
		to {
			transform: rotate(360deg);
		}
	}
	
	@-moz-keyframes roate360 {
		from {
			transform: rotate(0deg);
		}
		to {
			transform: rotate(360deg);
		}
	}
	
	@-webkit-keyframes name {
		from {
			transform: rotate(0deg);
		}
		to {
			transform: rotate(360deg);
		}
	}
	
	@-ms-keyframes name {
		from {
			transform: rotate(0deg);
		}
		to {
			transform: rotate(360deg);
		}
	}
