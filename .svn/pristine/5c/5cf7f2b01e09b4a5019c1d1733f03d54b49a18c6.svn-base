			var first = null;
			var old_back = mui.back;
			mui.back = function(){
				console.log('yes')
			}
			plus.key.addEventListener('backbutton', function() {		
				//首次按键，提示‘再按一次退出应用’  
				if(!first) {
					first = new Date().getTime();
					mui.toast('再按一次退出应用');
					setTimeout(function() {
						first = null;
					}, 2000);
